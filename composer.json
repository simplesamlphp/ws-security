{
    "name": "tvdijen/ws-trust",
    "description": "WS-Trust PHP library",
    "license": "LGPL-2.1-or-later",
    "authors": [
        {
            "name": "Tim van Dijen",
            "email": "tvdijen@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4 || ^8.0",
        "ext-openssl": "*",
        "ext-dom": "*",
        "ext-zlib": "*",

        "psr/log": "~1.1",
        "simplesamlphp/xml-security": "^0.4.3",
        "simplesamlphp/assert": "^0.2.13"
    },
    "require-dev": {
        "simplesamlphp/simplesamlphp-test-framework": "^1.1.7"
    },
    "autoload": {
        "psr-4": {
            "SimpleSAML\\WSTrust\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SimpleSAML\\Test\\WSTrust\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "v1.0.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    }
}
